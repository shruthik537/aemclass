package javaclass;

public class switchcase {
	public static void main(String[] args) {
		int a=10;
		switch (a) {
		case 10:
			System.out.println("a is integer value"+a);
			break;
		case 20:
			System.out.println("a is integer value"+a);
		
						break;

		default:
			System.out.println("default value");
			break;
		}
	}

}
